<!DOCTYPE html>
<html>
<head>
    @RenderSection("headbase", required: false)
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" type="text/css" href="~/libs/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/libs/bootstrap/css/bootstrap-theme.css">
    @RenderSection("head", required: false)
</head>
<body>
<nav class="navbar navbar-static-top navbar-default">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="~/">Angular 2 .net core</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
            <li><a href="~/angapp">Template</a></li>
            <li><a asp-action="register" asp-controller="account" >Register</a></li>
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</nav>
    <div class="container">
        @RenderBody()
        </div>
        <script src="~/libs/jquery/jquery.js"></script>
        <script src="~/libs/bootstrap/js/bootstrap.js"></script>
        @RenderSection("scripts", required: false)
</body>
</html>
